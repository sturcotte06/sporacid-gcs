body {
	background-color: #263238;
	font-family: Trebuchet MS;
	margin: 0px;
	padding: 0px;
	min-width: 1024px
}

.header-container {
	width: 90%;
	margin: 10px auto;
	margin-top: 20px;
	background-color: white;
	height: 90px;
	border-radius: 15px 15px 0px 0px;
	position: relative;
}

.header-container .header {
	font-weight: bold;
	font-family: century gothic;
	padding-top: 26px;
	padding-left: 24px;
	font-size: 32px;
}

.header-container .logo {
	background-image: url(../images/bender-seal-of-approval.png);
	background-size: 100% 100%;
	position: absolute;
	height: 150px;
	width: 150px;
	right: -30px;
	top: -20px;
	z-index: 1001;
}

.content-container {
	width: 90%;
	margin: 10px auto;
	position: relative;
}

.column-container {
  width: 20%;
  height: 100%;
  position: absolute;
  top: 0px;
}

.column-container.left-column {
  left: 0px;
}

.column-container.right-column {
  right: 0px;
}

.column-container .column-content {
  background-color: white;
  margin-bottom: 10px;
  /*min-height: 200px;*/
}

.column-content .content-header {
  background: #263238 url(../images/black_glass_back.png) no-repeat top left;
  background-size: 100% 100%;
  color: white;
  padding: 10px 0px;
  text-align: center;
  border: 3px solid white;
  cursor: pointer;
  margin-bottom: 0px;
}

.column-content:last-child {
	border-radius: 0px 0px 4px 4px;
}

.main-content {
	width: 58%;
	margin: auto;
	background-color: white;
	min-height: 100px;
	padding: 20px;
	border-radius: 0px 0px 4px 4px;
	-webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
	-moz-box-sizing: border-box;    /* Firefox, other Gecko */
	box-sizing: border-box;         /* Opera/IE 8+ */
}

.menu-container {
	width: 90%;
	margin: 10px auto;
	height: 40px;
	background-color: #263238;
}

.menu-container .menu {
	list-style-type: none;
	color: white;
	padding: 0px;
	margin: 0px;
	height: 40px;
}

.menu-container .menu .menu-item {
	display: block;
	float: left;
	width: 120px;
	text-align: center;
	border-right: 3px solid transparent;
	border-left: 3px solid transparent;
	cursor: pointer;
}

.menu-container .menu .menu-item.selected {
	border-right: 3px ridge white;
	border-left: 3px ridge white;
}

.menu-container .menu .menu-item:hover {
	border-right: 3px ridge white;
	border-left: 3px ridge white;
	background-image: url(../images/black_glass_back.png);
	background-size: 100% 100%;
}

.menu-container .menu .menu-item .menu-item-link {
	display: block;
	padding: 10px 0px;
	color: inherit;
	font-weight: inherit;
	font-style: inherit;
	font-size: inherit;
	text-decoration: inherit;
}

.form-line {
	padding: 3px 0px;
}

.form-label {
	display: inline-block;
	padding: 5px;
	color: white;
	background: #263238 url(../images/black_glass_back.png) no-repeat top left;
	background-size: 100% 100%;
	border-radius: 4px 0px 0px 4px;
	width: 110px;
	text-align: right;
	text-overflow: ellipsis;
	vertical-align: middle;
	border: 1px solid #263238;
}

.form-label.info-at-bot {
  border-radius: 4px 4px 0px 0px;
  display: block;
  margin-bottom: 5px;
  width: 100%;
  text-align: center;
}

.form-input {
	display: inline-block;
	margin: 0px;
	padding: 5px;
	font-size: 16px;
	border-radius: 0px 4px 4px 0px;
	border: 1px solid #263238;
	border-left: 5px solid #263238;
	vertical-align: middle;
	font-family: inherit;
	width: 200px;
}

.form-input {


.form-input:focus {
	border: 1px solid #CC5500;
	border-left: 5px solid #CC5500;
}

.form-input.read-only {
	border: 1px solid transparent;
	background: inherit;
	display: inline-block;
	width: auto;
}

.form-text-area {
	width: 100%;
	resize: vertical;
	margin-top: 10px;
	margin-bottom: 10px;
	padding: 5px;
	-webkit-box-sizing: border-box; /* Safari/Chrome, other WebKit */
	-moz-box-sizing: border-box;    /* Firefox, other Gecko */
	box-sizing: border-box;         /* Opera/IE 8+ */
}

.form-submit,
.form-button {
	padding: 5px;
	font-size: 16px;
	background: #263238 url(../images/black_glass_back.png) no-repeat top left;
	background-size: 100% 100%;
	color: white;
	width: 150px;
	border-radius: 4px;
	text-align: center;
	text-overflow: ellipsis;
	cursor: pointer;
}

.form-button .form-button-link {
	display: block;
	height: 100%;
	width: 100%;
	color: inherit;
	font-weight: inherit;
	font-style: inherit;
	font-size: inherit;
	text-decoration: inherit;
}

.form-image {
	height: 200px;
	width: 200px;
}

.form-link {
	color: #263238;
	font-style: underline;
	font-size: 12px;
}

.form-link:hover {
	color: black;
}

.form-information {
	display: inline-block;
	margin-left: 10px;
}

.form-edit {
	display: inline-block;
	background-image: url(../images/edit.png);
	background-size: 100% 100%;
	height: 24px;
	width: 24px;
	vertical-align: top;
	cursor: pointer;
}

.tab-container {
  margin: 10px auto;
  height: 30px;
  background-color: #263238;
  color: white;
  list-style-type: none;
  margin: 0px;
  padding: 0px;
  margin-bottom: 20px;
  border-radius: 4px;
}

.tab-container .tab-item {
	display: block;
  float: left;
  width: 120px;
  border-right: 3px solid transparent;
  border-left: 3px solid transparent;
  padding: 4px;
  cursor: pointer;
  text-align: center;
}

.tab-container .tab-item.selected {
  border-right: 3px ridge #CC5500;
  border-left: 3px ridge #CC5500;
}

.tab-container .tab-item:hover {
  border-right: 3px ridge #CC5500;
  border-left: 3px ridge #CC5500;
  background-image: url(../images/black_glass_back.png);
  background-size: 100% 100%;
}

.header1 {
	margin-bottom: 25px;
	font-size: 24px;
}

.header2 {
	margin-bottom: 20px;
	font-size: 20px;
}

.header3 {
	margin-bottom: 15px;
	font-size: 16px;
	font-weight: bold;
}

.messages .message {
	font-size: 14px;
	font-weight: bold;
	margin-bottom: 5px;
}

.messages .message:last-child {
	margin-bottom: 15px;
}

.messages {
	margin: 0px;
	list-style-type: lower-roman;
}

.messages .message.failure {
	color: Red;
}

.messages .message.success {
	color: Black;
}

.footer {
	padding: 10px;
	font-size: 10px;
	color: white;
	text-align: center;
}

.unselectable {
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: -moz-none;
	-ms-user-select: none;
	user-select: none;
}

.invisible {
	display: none;
}